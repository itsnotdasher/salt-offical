-- Animation effects
local TweenService = game:GetService("TweenService")

local function ButtonEffect(button)
    local originalSize = button.Size
    local originalPos = button.Position
    local originalColor = button.BackgroundColor3
    local originalTextColor = button.TextColor3
    local originalTransparency = button.TextTransparency or 0
    
    button.MouseEnter:Connect(function()
        if button.BackgroundTransparency < 1 then
            TweenService:Create(button, TweenInfo.new(0.1), {
                BackgroundColor3 = Color3.fromRGB(80, 140, 240),
                TextColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
            TweenService:Create(button:FindFirstChild("SubmitGlow") or Instance.new("Frame"), TweenInfo.new(0.1), {
                ImageColor3 = Color3.fromRGB(80, 140, 240)
            }):Play()
        else
            TweenService:Create(button, TweenInfo.new(0.1), {
                TextColor3 = Color3.fromRGB(255, 255, 255),
                TextTransparency = 0
            }):Play()
        end
    end)
    
    button.MouseLeave:Connect(function()
        if button.BackgroundTransparency < 1 then
            TweenService:Create(button, TweenInfo.new(0.1), {
                BackgroundColor3 = originalColor,
                TextColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
            TweenService:Create(button:FindFirstChild("SubmitGlow") or Instance.new("Frame"), TweenInfo.new(0.1), {
                ImageColor3 = originalColor
            }):Play()
        else
            TweenService:Create(button, TweenInfo.new(0.1), {
                TextColor3 = originalTextColor,
                TextTransparency = originalTransparency
            }):Play()
        end
    end)
    
    button.MouseButton1Down:Connect(function()
        if button.BackgroundTransparency < 1 then
            TweenService:Create(button, TweenInfo.new(0.05), {
                Size = originalSize - UDim2.new(0, 5, 0, 5),
                BackgroundColor3 = Color3.fromRGB(40, 90, 200)
            }):Play()
        end
    end)
    
    button.MouseButton1Up:Connect(function()
        if button.BackgroundTransparency < 1 then
            TweenService:Create(button, TweenInfo.new(0.1), {
                Size = originalSize,
                BackgroundColor3 = Color3.fromRGB(60, 110, 220)
            }):Play()
        end
    end)
end

-- Apply effects to buttons
ButtonEffect(SubmitButton)
ButtonEffect(GetKeyButton)
ButtonEffect(CloseButton)

-- Functionality
GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/wHqn5eGCsU")
    
    local notification = Instance.new("Frame")
    notification.Name = "Notification"
    notification.Size = UDim2.new(1, -40, 0, 40)
    notification.Position = UDim2.new(0, 20, 1, -60)
    notification.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    notification.BorderSizePixel = 0
    
    local notifText = Instance.new("TextLabel")
    notifText.Size = UDim2.new(1, 0, 1, 0)
    notifText.BackgroundTransparency = 1
    notifText.Font = Enum.Font.GothamMedium
    notifText.Text = "Discord link copied to clipboard!"
    notifText.TextColor3 = Color3.fromRGB(200, 200, 255)
    notifText.TextSize = 12
    notifText.Parent = notification
    
    local UICorner5 = Instance.new("UICorner")
    UICorner5.CornerRadius = UDim.new(0, 8)
    UICorner5.Parent = notification
    
    notification.Visible = false
    notification.Parent = MainFrame
    
    notification.Visible = true
    TweenService:Create(notification, TweenInfo.new(0.2), {Position = UDim2.new(0, 20, 1, -100)}):Play()
    
    wait(3)
    
    TweenService:Create(notification, TweenInfo.new(0.2), {Position = UDim2.new(0, 20, 1, -60)}):Play()
    wait(0.2)
    notification:Destroy()
end)

SubmitButton.MouseButton1Click:Connect(function()
    if KeyEntry.Text == "saltyer" then
        -- Correct key
        TweenService:Create(MainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 0, 0, 250)}):Play()
        wait(0.3)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/itsnotdasher/Voluted-Hub-V-1.1/refs/heads/main/balls"))()
        ScreenGui:Destroy()
    else
        -- Incorrect key
        local originalColor = KeyEntry.BackgroundColor3
        TweenService:Create(KeyEntry, TweenInfo.new(0.1), {
            BackgroundColor3 = Color3.fromRGB(80, 40, 40),
            TextColor3 = Color3.fromRGB(255, 150, 150)
        }):Play()
        wait(0.5)
        TweenService:Create(KeyEntry, TweenInfo.new(0.3), {
            BackgroundColor3 = originalColor,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        }):Play()
    end
end)

CloseButton.MouseButton1Click:Connect(function()
    TweenService:Create(MainFrame, TweenInfo.new(0.2), {Size = UDim2.new(0, 400, 0, 0)}):Play()
    wait(0.2)
    ScreenGui:Destroy()
end)

-- Initial animation
MainFrame.Size = UDim2.new(0, 0, 0, 0)
MainFrame.Visible = true
TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 400, 0, 250)
}):Play()

-- Add to player's GUI
local player = game.Players.LocalPlayer
ScreenGui.Parent = player:WaitForChild("PlayerGui")
